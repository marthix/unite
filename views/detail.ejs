<%- include('header') %>
  <% if (!loggedIn) { %>

  <% } else { %>
    <header class="header container-fluid">
      <nav class="container">
        <section class="row">
          <div class="column column-20">
            <a href="/"><img class="logo" src="assets/logos/unite-logo-black.png" height="40px"/></a>
            <a href="/"><img class="logo1" src="assets/logos/unite-logo-black-small.png" height="40px"/></a>
          </div>
          <div class="column nav">
            <div class="nav">
              <a class="selected" href="/directory">DIRECTORY</a>
            </div>
            <span class="">|</span>
            <div class="profile">
              <h6 class="username"><%= user.username_full %></h6>
              <img class="avatar" src="https://discordapp.com/api/users/<%= user.discord_id %>/avatars/<%= user.avatar %>.jpg" height="30px"/>
            </div>
          </div>
        </section>
      </nav>
    </header>
    <main class="container">
      <h3 class="game-header" id="game-title"></h3>
      <section class="row" id="teams">
        <div class="column column-33">
          <!-- <a href="/create?id=<%= query %>" class="create-team"> -->
          <a href="#" class="create-team">
            <div class="create-team-box">
              <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 490.2 490.2" class="plus-icon"><path class="plus-icon-path" d="M418.5 418.5c95.6-95.6 95.6-251.2 0-346.8s-251.2-95.6-346.8 0 -95.6 251.2 0 346.8S322.9 514.1 418.5 418.5zM89 89c86.1-86.1 226.1-86.1 312.2 0s86.1 226.1 0 312.2 -226.1 86.1-312.2 0S3 175.1 89 89z"/><path class="plus-icon-path" d="M245.1 336.9c3.4 0 6.4-1.4 8.7-3.6 2.2-2.2 3.6-5.3 3.6-8.7v-67.3h67.3c3.4 0 6.4-1.4 8.7-3.6 2.2-2.2 3.6-5.3 3.6-8.7 0-6.8-5.5-12.3-12.2-12.2h-67.3v-67.3c0-6.8-5.5-12.3-12.2-12.2 -6.8 0-12.3 5.5-12.2 12.2v67.3h-67.3c-6.8 0-12.3 5.5-12.2 12.2 0 6.8 5.5 12.3 12.2 12.2h67.3v67.3C232.8 331.4 238.3 336.9 245.1 336.9z"/></svg>
              <h5>Can't find a team?</h5>
              <h6>Create your own.</h6>
            </div>
          </a>
        </div>
      </section>
      <div style="visibility: hidden; height: 1px; overflow: hidden;" id="grabMe">

      </div>

    </main>
  <% } %>
  <script src="https://code.jquery.com/jquery-2.2.3.min.js"></script>
  <script src="http://code.jboxcdn.com/0.3.2/jBox.min.js"></script>
  <script type="text/javascript" src="js/jquery.radios-to-slider.js"></script>
  <script src="js/clipboard.js"></script>
  <script src="js/details.js"></script>
<%- include('footer') %>





